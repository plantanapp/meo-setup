{
  "Serilog": {
    "MinimumLevel": "Warning",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:/Logs/MultiEnv.log",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 5242880,
          "retainedFileCountLimit": 10
        }
      }
    ]
  },
  "GitSettings": {
    "JobQueue": {
      "JobRunnersCount": 3,
      "IdleQueueTimeToDeleteInMin": 10,
      "KeepFinishedJobsTimeInMin": 1440
    },
    "Operations": {
      "LocalFilesBaseDir": "C:/Repo",
      "Origin": "origin",
      "MainBranch": "main",
      "CommitterUsername": "plantanapp",
      "CommitterEmail": "contact@plantanapp.com",
      "InitialCommitDefaultMessage": "Initial Commit"
    }
  }
}
